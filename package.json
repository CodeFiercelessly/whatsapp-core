		"@babel/node": "^7.8.7",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.9.6",
		"app-root-path": "^3.0.0",
		"babel-eslint": "^10.1.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"cors": "^2.8.5",
		"curve25519-js": "^0.0.4",
		"dotenv": "^8.2.0",
		"eslint": "^7.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"express": "^4.17.1",
		"futoin-hkdf": "^1.3.3",
		"got": "^11.8.2",
		"https-proxy-agent": "^5.0.0",
		"husky": "^4.2.5",
		"instagram-private-api": "^1.44.0",
		"instagram_mqtt": "^1.0.2",
		"jimp": "^0.16.1",
		"jsonwebtoken": "^8.5.1",
		"lint-staged": "^10.2.4",
		"lodash": "^4.17.21",
		"mkdirp": "^1.0.4",
		"mongoose": "^5.12.0",
		"mongoose-aggregate-paginate-v2": "^1.0.42",
		"mongoose-paginate-v2": "^1.3.9",
