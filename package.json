{
	"name": "whatsapp-web",
	"version": "1.0.0",
	"description": "Extending WhatsApp Business",
	"main": "index.js",
	"scripts": {
		"start:api": "babel-node -- ./src/servers/api.js",
		"start:connection": "babel-node -- ./src/servers/api.js ${port} ${userId}",
		"start:worker": "babel-node -- ./src/servers/worker.js",
		"start:dev": "nodemon --exec babel-node -- ./src/servers/api.js",
		"prettier": "prettier --write 'src/**/*.{ts,js}'",
		"lint": "eslint ./src",
		"lint:fix": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix"
	},
	"keywords": [
		"Event Driven",
		"ES6",
		"NodeJS"
	],
	"author": "Joshua Ogunbo",
	"license": "ISC",
