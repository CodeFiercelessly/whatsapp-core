		"prettier": "^2.0.5",
		"prettierrc": "0.0.0-5",
		"protobufjs": "^6.10.2",
		"pusher": "^5.0.0",
		"qrcode-terminal": "^0.12.0",
		"ws": "^7.4.4"
	},
	"devDependencies": {},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{js,ts}": [
			"prettier --write",
			"eslint --fix src/"
		]
	},
	"nodemonConfig": {
		"ignore": [
			"src/**/*.json"
		]
	}
}
